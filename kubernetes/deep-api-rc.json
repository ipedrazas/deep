{
  "apiVersion": "v1",
  "kind": "ReplicationController",
  "metadata": {
    "name": "deep-api",
    "labels": {
      "name": "deep-api",
      "deep": "api"
    }
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "name": "deep-api",
        "labels": {
          "name": "deep-api",
          "deep": "api"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "deep-api",
            "image": "ipedrazas/deep-api:latest",
           "ports":[
                     {
                        "containerPort": 5000,
                        "protocol":"TCP"
                     }
                  ]
          }
        ]
      }
    }
  }
}
